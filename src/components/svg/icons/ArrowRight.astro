---
import type { HTMLAttributes } from "astro/types";
type Props = HTMLAttributes<"svg">;
---

{/* Source: https://phosphoricons.com/?q=arrow-right */}
<svg fill="none" viewBox="0 0 256 256" {...Astro.props}>
  <g
    stroke="currentColor"
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="16"
  >
    <line x1="40" y1="128" x2="216" y2="128"></line>
    <polyline points="144 56 216 128 144 200"></polyline>
  </g>
</svg>
